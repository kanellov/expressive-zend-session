{
    "name": "xorock/expressive-zend-session",
    "description": "Zend Session middleware for Zend Expressive",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "expressive",
        "http",
        "middleware",
        "psr",
        "psr-7",
        "zend",
        "session"
    ],
    "require": {
        "php": "^5.5 || ^7.0",
        "container-interop/container-interop": "^1.1",
        "psr/http-message": "^1.0",
        "zendframework/zend-session": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
      "psr-4": {
        "Mylab\\Session\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "MylabTest\\Session\\": "test/"
      }
    },
    "suggest": {
        "mtymek/expressive-config-manager": "Allows collecting and merging configuration from different sources"
    },
    "scripts": {
        "test": "phpunit"
    }
} 
